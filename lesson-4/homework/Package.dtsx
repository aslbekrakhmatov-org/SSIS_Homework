                string status = "Success";
                string message = "File processed successfully";
                DateTime logTime = DateTime.Now;

                string insertQuery = @"
                    INSERT INTO FileProcessingLog (FileName, Status, Message, LogTime)
                    VALUES (@FileName, @Status, @Message, @LogTime)";

                using (SqlConnection con = new SqlConnection("Data Source=Aslbek-Rakhmatovs-Laptop;Initial Catalog=ssis_4;Integrated Security=SSPI;"))
