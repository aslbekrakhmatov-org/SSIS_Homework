                Dts.Variables["User::errormessage"].Value = errorMessage;
                Dts.Variables["User::logtime"].Value = logTime;

                // Insert log to database
                string connStr = "Data Source=Aslbek-Rakhmatovs-Laptop;Initial Catalog=ssis_4;Integrated Security=SSPI;";
                string insertQuery = @"INSERT INTO FileProcessingLogFailure (FileName, Status, Message, LogTime)
                                       VALUES (@FileName, @Status, @Message, @LogTime)";

                using (SqlConnection con = new SqlConnection(connStr))
                {
                    SqlCommand cmd = new SqlCommand(insertQuery, con);
                    cmd.Parameters.AddWithValue("@FileName", fileName);
                    cmd.Parameters.AddWithValue("@Status", "Failed");
                    cmd.Parameters.AddWithValue("@Message", errorMessage);
                    cmd.Parameters.AddWithValue("@LogTime", logTime);

                    con.Open();
                    cmd.ExecuteNonQuery();
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                MessageBox.Show("Logging failed: " + ex.Message);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        enum ScriptResults
        {
            Success = DTSExecResult.Success,
            Failure = DTSExecResult.Failure
        };
    }
