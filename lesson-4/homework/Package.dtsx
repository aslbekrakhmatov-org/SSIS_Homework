                        cmd.Parameters.AddWithValue("@Status", status);
                        cmd.Parameters.AddWithValue("@Message", message);
                        cmd.Parameters.AddWithValue("@LogTime", logTime);

                        con.Open();
                        cmd.ExecuteNonQuery();
                        con.Close();
