            Encoding="UTF8"><![CDATA[#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.IO;
using System.Threading;

#endregion

namespace ST_b6aadf396c35403bb2058d6e3b9b6681
{

	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		public void Main()
		{
			string folderpath = Dts.Variables["User::folderpath"].Value.ToString();
            string filepattern = Dts.Variables["User::filename"].Value.ToString();
            bool fileexists = false;
            int maxretries = 5;
            int delay = 5000;

            try
            {
                for (int attempt = 1; attempt < maxretries; attempt++)
                {
                    if (Directory.Exists(folderpath))
                    {
                        string[] files = Directory.GetFiles(folderpath, filepattern);
                        if (files.Length > 0)
                        {
                            fileexists = true;
                            break;
                        }
                        else
                        {
                            Dts.Log("No files found matching: " + filepattern, 0, null);
                        }
                    }
                    else 
                    {
                        Dts.Log("Folder does not exist: " + folderpath, 0, null);
                        break;
                    }
                
                    if (attempt<maxretries && !fileexists)
                    {
                        System.Threading.Thread.Sleep(delay);
                        Dts.Log("Retry attempt " + attempt + " of " + maxretries, 0, null);
                    }                 
                }
                Dts.Variables["User::fileexists"].Value = fileexists;
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Log("Error: " + ex.Message, 0, null);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }






















            Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
