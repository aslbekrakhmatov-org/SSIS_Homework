            object resultSet = Dts.Variables["User::EmployeesFullSet"].Value;

            // Populate a DataTable with the result set
            System.Data.OleDb.OleDbDataAdapter adapter = new System.Data.OleDb.OleDbDataAdapter();
            DataTable dt = new DataTable();
            adapter.Fill(dt, resultSet);

            // Build a message with employee details
            string message = $"Found {dt.Rows.Count} employees with salary greater than average.\n\nEmployee Details:\n";
            if (dt.Rows.Count == 0)
            {
                message += "No employees found with salary greater than average.\n";
            }
            else
