using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    private int row_count = 0;

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        row_count++;
    }

    public override void PostExecute()
    {
        base.PostExecute();
        try
        {
            int max_rows = 2;

            if (row_count > max_rows)
            {
                Variables.CustomErrorCode = "ERR004";
                bool cancel = true;
                this.ComponentMetaData.FireError(0, "Script Component", $"Excessive number of rows: {row_count} exceeds limit of {max_rows}", string.Empty, 0, out cancel);
            }
            else 
            {
                Variables.CustomErrorCode = "SUCCESS";
            }
        }
        catch (Exception ex)
        {
            Variables.CustomErrorCode = "ERR099";
            bool cancel = true;
            this.ComponentMetaData.FireError(0, "Script Component", "Error in Script Component: " + ex.Message, string.Empty, 0, out cancel);
        }
    }

}
