            string packageName = Dts.Variables["System::PackageName"].Value.ToString();
            string taskName = Dts.Variables["System::TaskName"].Value.ToString();
            string errorDesc = Dts.Variables["System::ErrorDescription"].Value.ToString();
            string customErrorCode = Dts.Variables["User::CustomErrorCode"].Value.ToString();
            string logMessage = $"{DateTime.Now:yyyy-MM-dd HH:mm:ss} | Package: {packageName} | Task: {taskName} | Error: {errorDesc} | CustomErrorCode: {customErrorCode}";

