	{
		public void Main()
		{
            try
            {
                string packageName = Dts.Variables["System::PackageName"].Value.ToString();
                string errorMessage = Dts.Variables["System::ErrorDescription"].Value.ToString();
                string emailBody = $"An error occurred in SSIS package:\n\n" +
                                  $"Package Name: {packageName}\n" +
                                  $"Error Message: {errorMessage}\n" +
                                  $"Date: {DateTime.Now}";

                MailMessage mail = new MailMessage();
                SmtpClient smtpClient = new SmtpClient("smtp.gmail.com");
                mail.From = new MailAddress("aslbek.rakhmatov.us@gmail.com");
                mail.To.Add("rakhmatov.aslbek072@gmail.com");
                mail.Subject = "SSIS Package Error Notification";
                mail.Body = emailBody;

                smtpClient.Port = 587;
                smtpClient.Credentials = new System.Net.NetworkCredential("aslbek.rakhmatov.us@gmail.com", "didu sidx yjog peyl");
                smtpClient.EnableSsl = true;

                smtpClient.Send(mail);

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Script Task", ex.Message, String.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
